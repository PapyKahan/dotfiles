{
    "mcpServers": {
        "browser-server": {
            "command": "npx",
            "env": {
                "HTTP_PROXY": null,
                "HTTPS_PROXY": null,
                "NO_PROXY": null,
                "NODE_TLS_REJECT_UNAUTHORIZED": null
            },
            "args": [
                "-y",
                "@browsermcp/mcp@latest"
            ]
        },
        "git-server": {
            "command": "uvx",
            "args": [
                "mcp-server-git"
            ],
            "env": {
                "HTTP_PROXY": null,
                "HTTPS_PROXY": null,
                "NO_PROXY": null,
                "NODE_TLS_REJECT_UNAUTHORIZED": null
            }
        },
        "file-server": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-filesystem",
                "~/",
                "."
            ],
            "env": {
                "HTTP_PROXY": null,
                "HTTPS_PROXY": null,
                "NO_PROXY": null,
                "UV_NATIVE_TLS": null
            }
        },
        "fetch-server": {
            "command": "uvx",
            "args": [
                "mcp-server-fetch"
            ],
            "env": {
                "HTTP_PROXY": null,
                "HTTPS_PROXY": null,
                "NO_PROXY": null,
                "UV_NATIVE_TLS": null
            }
        }
    }
}
